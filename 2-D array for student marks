#include<stdio.h>
int main()
{
    int student,subject;
    printf("enter student number");
    scanf("%d",&student);
    printf("enter subject number");
    scanf("%d",&subject);
    int sm[student][subject];
    for(int i=0;i<student;i++){
        for(int j=0;j<subject;j++){
            printf("enter for student number %d and subject number %d::",i+1,j+1);
            scanf("%d",&sm[i][j]);
        }
    }
    //  for(int i=0;i<student;i++){
