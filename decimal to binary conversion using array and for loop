#include<stdio.h>
#include<math.h>
int main()
{
    int n,count=0;
    int A[10];
    printf("enter number::");
    scanf("%d",&n);
    for(int i=0;i<=100;i++){ 
        int a=(n/pow(2,i));
        
        A[i]=a%2;
        // printf("%d",A[i]);
         count=count+1;
        if(a==1||a==0){
            break;}
    }

  
    // printf("\n%d\n",count);
    for(int j=0;j<count;j++){
        printf("%d",A[count-j-1]);
    
    }
}
