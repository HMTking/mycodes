//count the number digit/reverce the number
#include<stdio.h>
#include<math.h>

int main()
{
    int n;
    printf("enter the three digit number");//if you want to enter more then three digit number just change condition of for loop i<=X here x is the number of digit in given number.
    scanf("%d",&n);
    for(int i=1;i<=3;i++)//
    {
        int a=(pow(10,i));
        int b=(pow(10,i-1));
        int c=n%a;
        int d=n%b;
        int e=pow(10,i-1);
     printf("%d",(c-d)/e);
    }
    
}
