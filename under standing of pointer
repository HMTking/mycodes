#include<stdio.h>
int main()
{
    int **po,*p,x=1;
    p=&x;
    po=&p;
// velue of pointer //
    printf("%u\n",p);
    printf("%u\n",po);
    printf("%u\n",*p);
    printf("%u\n",*po);
    printf("%u\n",**po);
// address of pointer //
    printf("%u\n",&x);
    printf("%u\n",&p);
    printf("%u\n",x);
    printf("%u\n",p);
    printf("%u\n",*(&x));// (* and & cancel each other)
}
